@model List<Hospitalproject.Models.SocialServiceClubs>
@{
    ViewBag.Title = "List of all Social Service Clubs";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <h2>List of Social Service Club</h2>
    <a href="/SocialServiceClubs/New">Add Social Service Club</a>
    <div class="col-lg-12">
        @{
            foreach (var ssc in Model)
            {
                <div class="col-md-4">
                    <h3><a href="/SocialServiceClubs/View/@ssc.SocialServiceClubs_id">@ssc.SocialServiceClubs_title</a></h3>
                    <p><a href="/SocialServiceClubs/View/@ssc.SocialServiceClubs_id">@ssc.SocialServiceClubs_details</a></p>
                    <p><a href="/SocialServiceClubs/View/@ssc.SocialServiceClubs_id">Ratings: @ssc.SocialServiceClubs_address</a></p>
                    

                    <p>
                        <a href="/SocialServiceClubs/Update/@ssc.SocialServiceClubs_id" class="btn btn-default" type="button">Update</a>
                        <a href="/SocialServiceClubs/Delete/@ssc.SocialServiceClubs_id" class="btn btn-default" type="button">Delete</a>
                    </p>
                </div>

            }

        }

    </div>
</body>
</html>

