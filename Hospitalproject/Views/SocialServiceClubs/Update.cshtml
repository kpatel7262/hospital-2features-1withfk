@model Hospitalproject.Models.SocialServiceClubs
@{
    ViewBag.Title = "Update Club information";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <h2>Edit Details for @Model.SocialServiceClubs_title</h2>
    <a href="/SocialServiceClubs/List">Back to List</a>
    <div>
        <form method="post" action="/SocialServiceClubs/Update/@Model.SocialServiceClubs_id">
            <div>
                <label for="SocialServiceClubs_title">Title:</label>
                <input type="text" name="SocialServiceClubs_title" id="SocialServiceClubs_title" value="@Model.SocialServiceClubs_title" />
            </div>
            <div>
                <label for="SocialServiceClubs_details">Details:</label>
                <input type="text" name="SocialServiceClubs_details" id="SocialServiceClubs_details" value="@Model.SocialServiceClubs_details" />
            </div>
            <div>
                <label for="SocialServiceClubs_address">Address:</label>
                <input type="text" name="SocialServiceClubs_address" id="SocialServiceClubs_address" value="@Model.SocialServiceClubs_address" />
            </div>
            <div class="controls">
                <input type="submit" value="Update" />
            </div>
        </form>

    </div>
</body>
</html>